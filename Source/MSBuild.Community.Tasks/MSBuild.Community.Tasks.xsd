          </xs:attribute>
          <xs:attribute name="Options" type="xs:string">
            <xs:annotation>
              <xs:documentation>[Optional] Regex options as strings corresponding to the RegexOptions enum:
                Compiled
                CultureInvariant
                ECMAScript 
                ExplicitCapture
                IgnoreCase
                IgnorePatternWhitespace
                Multiline
                None
                RightToLeft
                Singleline</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Output" type="xs:string">
            <xs:annotation>
              <xs:documentation>[Optional] [Output] Results of the Regex transformation.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
